{
  "manifest_version": 2,

  "name": "Submitify",
  "description": "Codeforces - Submit On Problem Page",
  "version": "2.0.0",
  "content_scripts": [
    {
      "js":["Submitify.js"],
      "matches":["https://codeforces.com/problemset/problem/*",
        "https://codeforces.com/contest/*/problem/*"]
  }
  ],
  "permissions": ["tabs"]
}